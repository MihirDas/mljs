{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["/Users/adamfowler/Documents/marklogic/git/jsdoc/plugins/markdown"],
"markdown": {
    "parser": "gfm",
    "hardwrap": true
},
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
